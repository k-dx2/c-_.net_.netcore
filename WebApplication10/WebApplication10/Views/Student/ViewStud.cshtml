@model List<WebApplication10.Models.Student>
@{
    ViewBag.Title = "ViewStud";
}

<h2>List of Stuents</h2>
<table style="width:100%">
    <tr>
        <th>Name</th>
        <th>Roll No</th>
        <th>Class</th>
        <th>Address</th>
    </tr>
    @foreach (var i in Model)
    {
        <tr>
            <td>@i.StudName</td>
            <td>@i.StudRollNo</td>
            <td>@i.StudClass</td>
            <td>@i.StudAddress</td>


        </tr>
    }


</table>

